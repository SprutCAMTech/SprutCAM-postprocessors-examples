<?xml version="1.0" encoding="utf-8"?>

<?xml-model href="PostSchema.xsd"?>

<Settings Version="1.0">
    <Description>
        <Comments />
        <CNCSystem />
        <MachineName />
        <Authors />
    </Description>
    <Common>
        <!-- No, Cuts, Halfs, Quaters -->
        <BreakArcs>No</BreakArcs>
        <!-- false, true -->
        <BreakHelix>false</BreakHelix>
        <ArcTolerance>0.01</ArcTolerance>
        <MaxCircleRadius>0</MaxCircleRadius>
        <DecimalSeparator>.</DecimalSeparator>
    </Common>
    <UserDefinedParameters>
        <Group ID="OutFiles" Caption="$(OutputFiles)">
            <Parameter ID="NCFileName" Caption="$(NCFileName)" Type="FileName" />
            <Parameter ID="NCProgName" Caption="NC program name (empty to get from the project name)" Type="String" />
            <Parameter ID="OutWCS" Caption="Output the WCS number at start" Type="Boolean" >true</Parameter>
            <Parameter ID="UseCycle19" Caption="Local CS ouput mode" Type="Boolean" >
                <Value Caption="PLANE SPATIAL" Default="true">false</Value>
                <Value Caption="CYCLE 19">true</Value>
            </Parameter>
        </Group>
    </UserDefinedParameters>
</Settings>
